{
    "data-root": "/storage/docker",
    "experimental": true,
    "ipv6": true,
    "ip6tables": true,
    "fixed-cidr-v6": "2001:db8:1::/64",
    "default-address-pools": [
      {
        "base": "10.100.0.1/16",
        "size": 24
      }
    ],
    "features": {
      "buildkit": true
    },
    "hosts": [
        "unix:///var/run/docker.sock",
        "tcp://[::]:2375"
    ],
    "mtu": 1400,
    "max-concurrent-downloads": 10,
    "max-concurrent-uploads": 5,
    "metrics-addr": "0.0.0.0:9323",
    "tls": false
}
